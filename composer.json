{
    "name": "fabricio872/api-modeller",
    "description": "Symfony bundle for translating foreign API to Doctrine-like models",
    "type": "symfony-bundle",
    "keywords": ["library", "api", "model"],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4|^8.0",
        "composer-fallback/php-http.client-implementation.guzzle7": "^1.1",
        "doctrine/annotations": "^1.13",
        "doctrine/collections": "^1.6",
        "symfony/http-kernel": "^4.4|^5",
        "symfony/serializer": "^4.4|^5",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.4|^5",
        "symplify/easy-coding-standard": "^9.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fabricio872\\ApiModeller\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Fabricio",
            "email": "fabricio872@gmail.com"
        }
    ],
    "scripts": {
        "test-psr": "vendor/bin/ecs check src",
        "fix-psr": "vendor/bin/ecs check src --fix"    }
}
