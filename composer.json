{
    "name": "fabricio872/api-modeller",
    "description": "Library for translating foreign API to Doctrine-like models",
    "type": "symfony-bundle",
    "keywords": [
        "library",
        "api",
        "model"
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "doctrine/annotations": "^1.13",
        "doctrine/collections": "^1.6",
        "symfony/http-client": "^4.4|^5",
        "symfony/property-access": "^4.4|^5",
        "symfony/serializer": "^4.4|^5",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symplify/easy-coding-standard": "^9.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fabricio872\\ApiModeller\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Fabricio",
            "email": "fabricio872@gmail.com"
        }
    ],
    "scripts": {
        "test-psr": "vendor/bin/ecs check src",
        "fix-psr": "vendor/bin/ecs check src --fix",
        "test": "vendor/phpunit/phpunit/phpunit --no-configuration tests/"
    }
}
